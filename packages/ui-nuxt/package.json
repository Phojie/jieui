{
  "name": "@nexvelt/ui-nuxt",
  "type": "module",
  "version": "0.0.2-beta.0",
  "description": "Nuxt module for @nexvelt/ui",
  "author": "Phojie Rengel <phojrengel@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/phojie",
  "homepage": "https://github.com/nexvelt/ui/tree/main/packages/ui-nuxt#readme",
  "repository": {
