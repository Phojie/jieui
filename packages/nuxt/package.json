{
  "name": "@una-ui/nuxt",
  "type": "module",
  "version": "0.21.0-beta.1",
  "description": "Nuxt module for @una-ui",
  "author": "Phojie Rengel <phojrengel@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/phojie",
  "homepage": "https://github.com/una-ui/una-ui/tree/main/packages/nuxt#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/una-ui/una-ui.git"
  },
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./una.config": {
      "import": "./dist/una.config.mjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "**/*.d.ts",
    "dist"
  ],
  "scripts": {
    "build": "nuxt-build-module",
    "stub": "nuxt-build-module build --stub && pnpm dev:prepare",
    "dev": "pnpm dev:prepare && nuxi dev playground",
    "dev:prepare": "nuxt-module-build prepare && nuxi prepare playground",
    "prepack": "nr build",
    "playground:build": "nuxi generate playground",
    "playground:preview": "nuxi preview playground"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.22",
    "@iconify-json/heroicons": "^1.2.0",
    "@iconify-json/lucide": "^1.2.2",
    "@iconify-json/radix-icons": "^1.2.0",
    "@iconify-json/tabler": "^1.2.0",
    "@nuxt/kit": "^3.13.1",
    "@nuxtjs/color-mode": "^3.4.4",
    "@tanstack/vue-table": "^8.20.5",
    "@una-ui/extractor-vue-script": "workspace:^",
    "@una-ui/preset": "workspace:^",
    "@unocss/core": "^0.62.4",
    "@unocss/nuxt": "^0.62.4",
    "@unocss/preset-attributify": "^0.62.4",
    "@unocss/preset-icons": "^0.62.4",
    "@vueuse/core": "^10.11.1",
    "@vueuse/integrations": "^10.11.1",
    "@vueuse/nuxt": "^10.11.1",
    "clsx": "^2.1.1",
    "ohash": "^1.1.3",
    "radix-vue": "^1.9.5",
    "tailwind-merge": "^2.5.2",
    "typescript": "^5.5.4",
    "unocss": "^0.62.4",
    "unocss-preset-animations": "^1.1.0"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.8.1",
    "@nuxt/schema": "^3.13.1",
    "nuxt": "^3.13.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
